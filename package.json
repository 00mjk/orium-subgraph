{
  "name": "OriumSubgraph",
  "version": "1.0.0",
  "author": "orium.network",
  "license": "MIT",
  "description": "A set of subgraphs (one for each network) for Orium Network dapps",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "graph-auth": "graph auth --product hosted-service",
    "codegen-matic": "graph codegen subgraph-matic.yaml",
    "codegen-bsc": "graph codegen subgraph-bsc.yaml",
    "codegen-mumbai": "graph codegen subgraph-mumbai.yaml",
    "build-matic": "graph build subgraph-matic.yaml",
    "build-bsc": "graph build subgraph-bsc.yaml",
    "build-mumbai": "graph build subgraph-mumbai.yaml",
    "deploy-matic": "graph deploy --node https://api.thegraph.com/deploy/ orium-network/orium-polygon subgraph-matic.yaml",
    "deploy-temp": "graph deploy --node https://api.thegraph.com/deploy/ orium-network/temporary-subgraph subgraph-matic.yaml",
    "deploy-bsc": "graph deploy --node https://api.thegraph.com/deploy/ orium-network/orium-bsc subgraph-bsc.yaml",
    "deploy-mumbai": "graph deploy --node https://api.thegraph.com/deploy/ orium-network/orium-mumbai subgraph-mumbai.yaml",
    "test-mumbai": "graph test mumbai",
    "test-matic": "graph test matic"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OriumNetwork/orium-subgraph.git"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.33.0",
    "gluegun": "^5.1.2"
  },
  "devDependencies": {
    "@graphprotocol/graph-ts": "^0.27.0",
    "matchstick-as": "^0.5.0"
  }
}
